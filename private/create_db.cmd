sqlite3 games.db "CREATE TABLE games (game_id TEXT PRIMARY KEY, player1 TEXT, player2 TEXT); CREATE TABLE players (game_id TEXT, player_id TEXT, x INTEGER, y INTEGER, angle REAL, bullets TEXT, PRIMARY KEY (game_id, player_id)); CREATE TABLE bullets (bullet_id TEXT PRIMARY KEY, game_id TEXT, x REAL, y REAL, direction REAL, created_at TIMESTAMP, FOREIGN KEY(game_id) REFERENCES games(game_id)); CREATE TABLE squares (game_id TEXT PRIMARY KEY, x INTEGER, y INTEGER, is_visible INTEGER DEFAULT 1, FOREIGN KEY(game_id) REFERENCES games(game_id));"